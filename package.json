{
  "name": "goldman",
  "version": "1.0.1",
  "description": "\t使用jquery + es6实现的黄金矿工小游戏",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "eslint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore --cache --ext .js,.html ./src --fix",
    "dev": "node build/dev-server.js",
    "dll": "webpack --config build/webpack.dll.config.js",
    "build": "node build/build.js",
    "build:deploy": "node build/build.js --deploy",
    "postinstall": "npm run dll",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "*.{js,html}": [
      "npm run eslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Yechuanjie/goldman.git"
  },
  "author": "YCJ",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "es6-promise": "^4.2.4",
    "gh-pages": "^2.0.1",
    "husky": "^0.15.0-rc.13",
    "jquery": "^3.2.1",
    "js-base64": "^2.4.9",
    "lint-staged": "^7.0.0",
    "swiper": "^4.2.6"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.1",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
